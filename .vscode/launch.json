{
	"version": "0.2.0",

	"configurations": [
		{
			"command": "pnpm --filter fastcore generate-auth-models",
			"name": "Launch Program",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "Attach by Process ID",
			"processId": "${command:PickProcess}",
			"request": "attach",
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		}
	]
}
